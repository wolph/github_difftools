{
    "name": "Github Difftools",
    "version": "0.9",
    "manifest_version": 2,
    "description": "Adding more diff options so you can easily compare 2 commits/tags/branches",
    "content_scripts": [{
        "matches": [
            "https://github.com/*",
            "http://github.com/*"
        ],
        "js": [
            "jquery.min.js",
            "github_difftools.js"
        ],
        "run_at": "document_end"
    }],
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
